{
  "name": "angular-msal",
  "author": {
    "name": "Marcel Hoekstra"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcelh1983/msal-angular-clone.git"
  },
  "version": "1.0.2",
  "keywords": [
    "implicit",
    "angular",
    "AAD",
    "msal",
    "oauth"
  ],
  "main": "./dist/index.js",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "whitelistedNonPeerDependencies": ["."]
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish": "ng-packagr -p package.json && npm publish ./dist"
  },
  "typings": "./dist/index.d.ts",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "msal": "~1.0.0"
  },
  "devDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "ng-packagr": "^5.1.0",
    "codelyzer": "~5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.5.0",
    "tslint": "^5.15.0",
    "typescript": "3.1.6",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {
    "@types/es6-shim": "^0.31.39",
    "@angular/common": ">= 6.0.0",
    "@angular/core": ">= 6.0.0",
    "@angular/router": ">= 6.0.0",
    "rxjs": ">= 6.3.0"
  },
  "directories": {
    "test": "tests"
  },
  "description": "@azure/angular-msal clone with support for Angular 6+ and Internet Explorer"
}
