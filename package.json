{
  "name": "angular-msal",
  "author": {
    "name": "Marcel Hoekstra"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcelh1983/angular-msal.git"
  },
  "version": "1.1.1",
  "keywords": [
    "implicit",
    "angular",
    "AAD",
    "msal",
    "oauth"
  ],
  "main": "./package/index.js",
  "ngPackage": {
    "lib": {
      "entryFile": "modules/index.ts",
      "umdModuleIds": {
        "msal": "msal"
      }
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "scripts": {
    "build": "rimraf dist && ng-packagr -p package.json",
    "publish-beta": "npm run build && npm publish ./dist --tag beta",
    "publish": "npm run build && npm publish ./dist",
    "start": "ng serve --port 4201"
  },
  "typings": "./dist/index.d.ts",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "msal": "~1.3.2"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.40",
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/common": "^10.0.3",
    "@angular/compiler": "^10.0.3",
    "@angular/compiler-cli": "^10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/platform-browser": "^10.0.3",
    "@angular/platform-browser-dynamic": "^10.0.3",
    "@angular/router": "^10.0.3",
    "ng-packagr": "^10.0.0",
    "bootstrap": "4.5.0",
    "codelyzer": "~6.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "eslint": "^7.4.0",
    "typescript": "3.9.6",
    "zone.js": "^0.10.3"
  },
  "peerDependencies": {},
  "directories": {
    "test": "tests"
  },
  "description": "msal with Angular using msal@1.x, with support for Angular6+ and es5"
}
