{
  "name": "angular-msal",
  "author": {
    "name": "Marcel Hoekstra"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcelh1983/angular-msal.git"
  },
  "version": "1.1.0-beta.0",
  "keywords": [
    "implicit",
    "angular",
    "AAD",
    "msal",
    "oauth"
  ],
  "main": "./package/index.js",
  "ngPackage": {
    "lib": {
      "entryFile": "modules/index.ts",
      "umdModuleIds": {
        "msal": "msal"
      }
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "scripts": {
    "build": "rimraf dist && ng-packagr -p package.json",
    "publish-beta": "npm run build && npm publish ./dist --tag beta",
    "publish": "npm run build && npm publish ./dist",
    "start": "ng serve --port 4201"
  },
  "typings": "./dist/index.d.ts",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "msal": "~1.2.1"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.39",
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "ng-packagr": "^9.0.0",
    "bootstrap": "4.4.1",
    "codelyzer": "~5.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "eslint": "^6.8.0",
    "typescript": "3.7.5",
    "zone.js": "^0.10.2"
  },
  "peerDependencies": {
    "@angular/common": ">= 6.0.0",
    "@angular/core": ">= 6.0.0",
    "@angular/router": ">= 6.0.0",
    "rxjs": ">= 6.3.0"
  },
  "directories": {
    "test": "tests"
  },
  "description": "msal with Angular using msal@1.x, with support for Angular6+ and es5"
}
